  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heiiab/hello-next/issues"
  },
  "homepage": "https://github.com/heiiab/hello-next#readme",
  "dependencies": {
    "antd": "^4.15.5",
    "next": "^10.2.0",
    "next-i18next": "^8.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/less": "^3.0.2",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@zeit/next-css": "^1.0.1",
    "classnames": "^2.3.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "7.3.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^11.0.0",
    "null-loader": "^4.0.1",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4",
    "webpack": "4.46.0"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "browserslist": [
    ">0.3%",
    "not ie <= 11",
    "not dead",
